// Vec validation test program
// This program MUST work completely after Vec implementation

func main() {
    // Test basic Vec creation and operations
    print("Starting Vec test");
    let vec = Vec::new();
    print("Vec created");
    
    // Test Vec push
    let mut vec2 = Vec::new();
    vec2.push(42);
    print("Element pushed");
    
    // Test Vec length
    let len = vec2.len();
    if (len == 1) {
        print("Length correct");
    }
    
    // Test Vec get
    let val = vec2.get(0);
    if (val == 42) {
        print("Value correct");
    }
    
    print("Vec test completed");
}