func fibonacci(n: i32) -> i32 {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

func main() -> () {
    let result: i32 = fibonacci(10);
    print("Fibonacci(10) computed successfully!");
    return;
}

## Test 2: Advanced Expression Parsing
# Save as: complex_math.eÃ¤  
func complex_calculation() -> i32 {
    let a: i32 = 5;
    let b: i32 = 10;
    let c: i32 = 3;
    
    let result: i32 = (a + b) * c - (a * 2) + (b / 2);
    return result;
}

func main() -> () {
    let answer: i32 = complex_calculation();
    print("Complex math works!");
    return;
}