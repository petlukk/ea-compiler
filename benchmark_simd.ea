func simd_operations() {
    let vec1 = [1.0, 2.0, 3.0, 4.0]f32x4;
    let vec2 = [5.0, 6.0, 7.0, 8.0]f32x4;
    
    // Perform 100000 SIMD operations
    for (let i: i32 = 0; i < 100000; i += 1) {
        let sum = vec1 .+ vec2;
        let product = vec1 .* vec2;
        let diff = vec1 .- vec2;
    }
    
    println("SIMD operations completed");
}

func main() {
    simd_operations();
}