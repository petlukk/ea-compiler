// Debug dot product
func main() -> () {
    let vec1 = [1.0, 2.0, 3.0, 4.0]f32x4;
    let vec2 = [2.0, 2.0, 2.0, 2.0]f32x4;
    
    let result = dot_product(vec1, vec2);
    
    // Check if it's close to 20.0 (floating point precision)
    if (result > 19.9 && result < 20.1) {
        print("Dot product is approximately 20.0");
    } else {
        print("Dot product is not 20.0");
    }
    
    return;
}