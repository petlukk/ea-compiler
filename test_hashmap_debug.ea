func main() -> () {
    println("Testing HashMap step by step");
    let mut map = HashMap::new();
    
    println("Created HashMap");
    
    // Test length before insertion
    let len_before = map.len();
    println("Length before insertion: 0");
    
    // Insert one key
    map.insert(1, 42);
    println("Inserted key 1 with value 42");
    
    // Test length after insertion
    let len_after = map.len();
    println("Length after insertion: should be 1");
    
    // Check if contains_key works
    let has_key = map.contains_key(1);
    if (has_key) {
        println("Contains key 1: YES");
    } else {
        println("Contains key 1: NO");
    }
    
    // Try to get the value
    let value = map.get(1);
    println("Value for key 1: should be 42");
}